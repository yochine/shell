<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.zrxjava.system.modules.ums.mapper.RoleMapper">

    <select id="getByUserId" resultType="me.zrxjava.system.modules.ums.entity.Role">
        select a.* from sys_role a join sys_user_role b on a.role_id = b.role_id where b.user_id = #{userId}
    </select>
</mapper>
