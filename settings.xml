<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <servers>
        <server>
            <id>zrxjava-zhong-cloud-mvnrepo</id>
            <username>${env.CODING_ARTIFACTS_USERNAME}</username>
            <password>${env.CODING_ARTIFACTS_PASSWORD}</password>
        </server>
    </servers>



    <!-- omitted xml -->
    <profiles>
        <profile>
            <id>Repository Proxy</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>zrxjava-zhong-cloud-mvnrepo</id>
                    <name>mvnrepo</name>
                    <url>https://zrxjava-maven.pkg.coding.net/repository/zhong-cloud/mvnrepo/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>